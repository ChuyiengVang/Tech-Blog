{{#each userPosts as |post|}}
<section>
    <div class="posts">
      <div>
        <div>
          <h2>{{title}}</h2>
        </div>
        <div>
            <a>Posted by {{user.name}}</a>
        </div>
        <div>
            <p>
                {{content}}
            </p>
        </div>
      </div>
    </div>
</section>
{{/each}}